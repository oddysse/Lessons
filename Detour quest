namespace Lesson7
{
    class Program
    {
        static void Main(string[] args)
        {
            for (int i = 1; i <= 5; i++)
            {
                Console.WriteLine("Level: " + i);
                for (int j = 0; j <= i; j += 2)
                {
                    Console.WriteLine("enemies " + j);
                }
            }
        }
    }
}
