using System;
using System.Diagnostics;

class Program
{
    static void Main()
    {
        int hp = 100;
        int healed = Heal(hp);

        Console.WriteLine($"Expected > {hp}, Actual: {healed}");
        Debug.Assert(healed > hp, "Heal() did not increase HP as expected.");
    }

    static int Heal(int hp)
    {
        return hp + new Random().Next(10, 21);
    }
}
