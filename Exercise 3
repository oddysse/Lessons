using System;

namespace LessonExercise3
{
    class Program
    {
        static void Main(string[] args)
        {
            for (int i = 1; i <= 5; i++)
            {
                Console.WriteLine(i);
                if (i >= 1)
                {
                    Console.WriteLine("Loading Level");

                }

            }

        }    
    }
}
