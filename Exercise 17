using System;
using System.Threading;
using System.Collections.Generic;

class Program
{
    static void Main(string[] args)
    {
        Dictionary<string, int> weaponDamage = new Dictionary<string, int>();

        weaponDamage.Add("sword", 12);
        weaponDamage.Add("hammer", 18);
        weaponDamage.Add("spear", 15);

        foreach (var weapon in weaponDamage)
        {
            Console.WriteLine($"{weapon.Key} deals {weapon.Value} damage");
        }
    }
}
