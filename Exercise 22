using System;
using System.IO;
using System.Collections.Generic;

class Program
{
    static void Main(string[] args)
    {
        List<string> inventory = new List<string>
        {

            "player",
            "enemy",
            "item",
            "health",
            "stamina"

        }; 

        File.WriteAllLines("inventory.txt", inventory);

        Console.WriteLine("Inventory was saved to file.");
    }
}
